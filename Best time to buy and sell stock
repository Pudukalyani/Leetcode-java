import java.lang.Math;
class Solution {
    public int maxProfit(int[] prices) {
        int curr = Integer.MAX_VALUE;
        int maxprofit = Integer.MIN_VALUE;
        for(int i = 0; i < prices.length; i++) {
            if(curr > prices[i]) {
                curr = prices[i];
            }
            maxprofit  = Math.max(maxprofit, prices[i] - curr);

        }
        return maxprofit;
    }
}
