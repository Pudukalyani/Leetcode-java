class Solution {
    public int majorityElement(int[] nums) {
        int maj = 0;
        int count = 0;
        for(int num: nums) {
            if(count  == 0) {
                maj = num;
            } 
            if (maj == num) {
                count++;
            } else {
                count--;
            }
        }
        return maj;
    }
}
